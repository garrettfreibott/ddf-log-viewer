{
  "name": "log-viewer",
  "version": "0.0.0",
  "description": "views logs",
  "main": "index.js",
  "scripts": {
    "pretest": "standard",
    "test": "browserify -d test/*.js | tape-run | tap-nyan",
    "watch": "onchange '*.js' 'test/**/*.js' -- npm t",
    "start": "hotloadify index.js --open"
  },
  "keywords": [
    "log",
    "viewer"
  ],
  "author": "Garrett",
  "license": "MIT",
  "dependencies": {
    "color": "^0.11.1",
    "keymaster": "^1.6.2",
    "merge": "^1.2.0",
    "moment": "^2.11.2",
    "random-item": "^1.0.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "reactify": "^1.1.1",
    "redux": "^3.3.1"
  },
  "devDependencies": {
    "browserify": "^13.0.0",
    "hotloadify": "0.0.4",
    "onchange": "^2.1.2",
    "react-addons-test-utils": "^0.14.7",
    "react-stateless-wrapper": "^1.0.2",
    "standard": "^6.0.8",
    "tap-nyan": "0.0.2",
    "tape": "^4.5.1",
    "tape-run": "^2.1.3"
  },
  "browserify": {
    "transform": [
      "reactify"
    ]
  }
}
